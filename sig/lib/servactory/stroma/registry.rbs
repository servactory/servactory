module Servactory
  module Stroma
    class Registry
      include Singleton

      class Entry < Data
        attr_reader key: Symbol
        attr_reader mod: Module

        def self.new: (key: Symbol, mod: Module) -> Entry
      end

      @entries: Array[Entry]
      @keys_index: Hash[Symbol, Entry]
      @finalized: bool

      def self.register: (Symbol key, Module mod) -> void
      def self.finalize!: () -> void
      def self.find: (Symbol key) -> Entry?
      def self.entries: () -> Array[Entry]
      def self.keys: () -> Array[Symbol]
      def self.finalized?: () -> bool

      def initialize: () -> void

      def register: (Symbol key, Module mod) -> void

      def finalize!: () -> void

      def entries: () -> Array[Entry]

      def find: (Symbol key) -> Entry?

      def keys: () -> Array[Symbol]

      def finalized?: () -> bool

      private

      def ensure_finalized!: () -> void
    end
  end
end
