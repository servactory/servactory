module Servactory
  module TestKit
    module Rspec
      module Matchers
        module Submatchers
          module Shared
            class InclusionSubmatcher < Base::Submatcher
              OPTION_NAME: Symbol

              OPTION_BODY_KEY: Symbol

              @values: (Range[untyped] | Array[untyped])
              @actual_values: (Range[untyped] | Array[untyped])
              @attribute_inclusion: untyped
              @attribute_inclusion_in: (Range[untyped] | Array[untyped] | nil)

              def initialize: (Base::SubmatcherContext context, (Range[untyped] | Array[untyped]) values) -> void

              def description: () -> String

              private

              attr_reader values: (Range[untyped] | Array[untyped])

              def passes?: () -> bool

              def build_failure_message: () -> String

              def attribute_inclusion: () -> untyped

              def attribute_inclusion_in: () -> (Range[untyped] | Array[untyped] | nil)

              def inclusion_values_match?: ((Range[untyped] | Array[untyped]) actual, (Range[untyped] | Array[untyped]) expected) -> bool
            end
          end
        end
      end
    end
  end
end
