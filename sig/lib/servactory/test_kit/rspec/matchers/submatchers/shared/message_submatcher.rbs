module Servactory
  module TestKit
    module Rspec
      module Matchers
        module Submatchers
          module Shared
            class MessageSubmatcher < Base::Submatcher
              @custom_message: (String | Proc)?
              @actual_message: String?
              @option_name: Symbol?

              def initialize: (Base::SubmatcherContext context, (String | Proc)? custom_message) -> void

