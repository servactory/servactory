module Servactory
  module TestKit
    module Rspec
      module Matchers
        module Base
          class Submatcher
            include ::RSpec::Matchers::Composable
            include Concerns::AttributeDataAccess
            include Concerns::ErrorMessageBuilder
            include Concerns::ValueComparison

            @context: SubmatcherContext
            @missing_option: String

            attr_reader missing_option: String

            def initialize: (SubmatcherContext context) -> void

            def matches?: (untyped subject) -> bool

            def failure_message: () -> String

            def failure_message_when_negated: () -> String

            def description: () -> String

            private

            def passes?: () -> bool

            def build_failure_message: () -> String

            attr_reader context: SubmatcherContext
          end
        end
      end
    end
  end
end
