module Servactory
  module Exceptions
    class Input < Base
      attr_reader service: Servactory::Context::Workspace::Actor
      attr_reader message: String
      attr_reader input_name: Symbol?
      attr_reader meta: untyped?

      def initialize: (
        context: Servactory::Context::Workspace,
        message: String,
        ?input_name: Symbol?,
        ?meta: untyped?
      ) -> void

      private

      # API: Datory
      attr_reader context: Servactory::Context::Workspace
    end
  end
end
