module Servactory
  module Outputs
    module DSL
      def self.included: (untyped base) -> void

      module ClassMethods
        @collection_of_outputs: Collection

        attr_reader config: Servactory::Configuration::Setup

        def inherited: (untyped child) -> void

        private

        def output: (Symbol name, *Symbol helpers, **untyped options) -> void

        def collection_of_outputs: () -> Collection
      end
    end
  end
end
